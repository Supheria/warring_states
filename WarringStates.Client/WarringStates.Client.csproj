      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Component\Overview.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Component\Overview.Op.cs" />
    <Compile Update="Component\Settings.cs">

<Control
    x:Class="WarringStates.Server.GUI.Views.Thumbnail"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:vm="using:WarringStates.Server.GUI.ViewModels"
    d:DesignHeight="450"
    d:DesignWidth="800"
    x:DataType="vm:ThumbnailViewModel"
    BackColor="{Binding BackColor}"
	FrontColor="{Binding FrontColor}"
    Bounds="{Binding Bounds, Mode=OneWayToSource}"
    Source="{Binding Source}"
	BorderThickness="{Binding BorderThickness}"
    mc:Ignorable="d" />
